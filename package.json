{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "build:api": "turbo run build --filter={/apps/api}",
    "build:web": "turbo run build --filter={/apps/web}",
    "build:doc": "turbo run build --filter={/apps/doc}",
    "dev:api": "turbo run dev --filter={/apps/api}",
    "dev:web": "turbo run dev --filter={/apps/web}",
    "dev:doc": "turbo run dev --filter={/apps/doc}",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "locations-api-venezuela-repo"
}
